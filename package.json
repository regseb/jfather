{
  "name": "jfather",
  "version": "0.3.0",
  "description": "JSON with merge, extend and override.",
  "keywords": [
    "jfather",
    "json",
    "merge",
    "extend",
    "override",
    "front-end",
    "backend"
  ],
  "homepage": "https://github.com/regseb/jfather#readme",
  "bugs": {
